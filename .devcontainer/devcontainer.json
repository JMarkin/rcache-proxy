    },
    "docker.host": "tcp://192.168.88.251:2375",
  },
  "extensions": [
    "hdevalke.rust-test-lens",
    "swellaby.vscode-rust-test-adapter",
    "matklad.rust-analyzer",
    "bungcip.better-toml",
    "vadimcn.vscode-lldb",
    "mutantdino.resourcemonitor",
    "jscearcy.rust-doc-viewer",
    "serayuzgur.crates",
    "k--kato.intellij-idea-keybindings",
  ],
  "forwardPorts": [],
}
